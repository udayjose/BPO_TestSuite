{
    "title": "Login Flow",
    "steps": [
        {
            "type": "waitForElement",
            "selectors": [
                "body"
            ]
        },
        {
            "type": "setViewport",
            "width": 1365,
            "height": 945,
            "deviceScaleFactor": 1,
            "isMobile": false,
            "hasTouch": false,
            "isLandscape": false
        },
        {
            "type": "click",
            "description": "If logout button exists, click it to reset state",
            "target": "main",
            "selectors": [
                [
                    "text/Logout"
                ],
                [
                    "header button"
                ]
            ],
            "optional": true
        },
        {
            "type": "navigate",
            "assertedEvents": [
                {
                    "type": "navigation",
                    "url": "https://birthdayhub-2.preview.emergentagent.com/",
                    "title": "Emergent | Fullstack App"
                }
            ],
            "url": "https://birthdayhub-2.preview.emergentagent.com/"
        },
